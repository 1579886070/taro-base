{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "jsx": "react",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@/components/*": ["./src/components/*"],
      "@/common/*": ["./src/common/*"],
      "@/config/*": ["./src/config/*"],
      "@/service/*": ["./src/service/*"],
      "@/stores/*": ["./src/stores/*"],
      "@/assets/*": ["./src/assets/*"],
      "@/utils/*": ["./src/utils/*"],
      "@/servers/*": ["./src/servers/*"],
    }
  },
  "include": ["./src", "global.d.ts"],
  "compileOnSave": false
}
